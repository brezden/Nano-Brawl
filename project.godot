config_version=5

[application]

config/name="Nano Brawl"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.1", "C#", "GL Compatibility")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=288
window/size/viewport_height=288
window/size/window_width_override=2560
window/size/window_height_override=1440
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="Nano Brawl"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
